<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<html>
	<head>
		<meta charset="utf-8">
		<meta name="description" content="">
		<meta name="viewport" content="width=device-width">
		<%=csrf_meta_tags %>

		<title>Sharingstuff</title>
		
		<!-- CSS -->
  		<%=stylesheet_link_tag "application", :media => "all" %>
		<%=controller_stylesheet_link_tag %>
		<%=action_stylesheet_link_tag %>
		
		<!-- Javascript -->
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=true&amp;libraries=places"></script>
		<%=javascript_include_tag "application" %>
		<%=controller_javascript_include_tag %>
		<%=action_javascript_include_tag %>
	</head>
	<body>
		<!-- WRAPPER -->
		<div class="wrapper">
			<!-- HEADER -->
			<%=render 'layouts/header' %>
			<!-- END HEADER -->
			
			<!-- NAV -->
			<%=render 'layouts/navigation' %>
			<!-- END NAV -->
			
			<!-- MAIN -->
			<%=yield %>
			<!-- END MAIN -->
			
			<!-- FOOTER -->
			<%=render 'layouts/footer' %>
			<!-- END FOOTER -->
		</div>
	</body>
</html>
